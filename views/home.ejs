
<%- include("partials/header"); -%>

    <h1>Home</h1>
    
    <p> <%= startingContent %> </p>


  <%  posts.forEach(function(post){ %>
  <div class="contentContainer">
    <div class="content">
      <h1><%=post.title%></h1>
      <p>
      <%=post.content.substring(0, 200) + " ..."%>
      <a href="/posts/<%=post._id%>">Read More</a>
      </p>
    </div>
  </div>

    <% }) %>
  


<%- include("partials/footer"); -%>
